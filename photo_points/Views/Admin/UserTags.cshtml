@model IEnumerable<photo_points.Models.UserTag>
@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "UserTags";
}

<h2>UserTags</h2>

<h3>Edit Tags</h3>
<div>
    @foreach (UserTag item in Model)
    {
        <h4>UserTag</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(item => item.Tag.tagName)
            </dt>
            <dt>
                @Html.DisplayNameFor(item => item.User.firstName)
            </dt>
        </dl>
    }
</div>
